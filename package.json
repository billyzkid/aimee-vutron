{
  "private": true,
  "name": "aimee-vutron",
  "version": "0.0.1",
  "license": "Unlicense",
  "productName": "AImee",
  "description": "Meet AImee, your artificially intelligent best friend!",
  "keywords": [
    "Electron",
    "TypeScript",
    "ChatGPT API",
    "Speech SDK",
    "Cubism SDK",
    "OpenJS",
    "OpenAI",
    "Live2D",
    "Microsoft",
    "Azure Cognitive Services"
  ],
  "author": "Will Allan <billyzkid@yahoo.com> (https://github.com/billyzkid)",
  "homepage": "https://github.com/billyzkid/aimee-vutron#readme",
  "bugs": "https://github.com/billyzkid/aimee-vutron/issues",
  "repository": "github:billyzkid/aimee-vutron",
  "main": "./dist/main",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "deploy": "vite build",
    "deploy:debug": "vite build --debug",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test": "vitest run main preload renderer",
    "test:e2e": "npm run build && vitest run e2e",
    "build": "npm-run-all clean lint typecheck deploy && electron-builder",
    "build:dir": "npm-run-all clean lint typecheck deploy && electron-builder --dir",
    "clean": "bash ./scripts/clean.sh",
    "reset": "bash ./scripts/reset.sh"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@mdi/font": "^7.2.96",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuetify": "^3.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.3.2",
    "electron": "^24.3.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.12.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.33.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.31.0",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.6.4"
  }
}

{
  "private": true,
  "name": "aimee-vutron",
  "version": "0.0.1",
  "description": "Meet AImee, your artificially intelligent best friend!",
  "keywords": [
    "Electron",
    "TypeScript",
    "ChatGPT API",
    "Speech SDK",
    "Cubism SDK",
    "OpenJS",
    "OpenAI",
    "Live2D",
    "Microsoft",
    "Azure Cognitive Services"
  ],
  "author": "Will Allan <billyzkid@yahoo.com> (https://github.com/billyzkid)",
  "homepage": "https://github.com/billyzkid/aimee-vutron#readme",
  "bugs": "https://github.com/billyzkid/aimee-vutron/issues",
  "repository": "github:billyzkid/aimee-vutron",
  "license": "Unlicense",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "dev:force": "vite --force",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier . --check --loglevel=silent",
    "format:fix": "prettier . --write --loglevel=silent",
    "prebuild": "npm run lint && npm run format && vite build",
    "build": "electron-builder --config ./electron-builder.config.json",
    "build:mac": "npm run build -- --mac",
    "build:linux": "npm run build -- --linux",
    "build:win": "npm run build -- --win"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "pinia": "^2.0.35",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuetify": "^3.2.2"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "electron": "^24.1.3",
    "electron-builder": "^23.3.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.6.2"
  }
}
